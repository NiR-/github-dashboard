        "memio/memio": "~2@dev",
        "memio/linter": "~2@dev",
        "memio/model": "~2@dev",
        "memio/pretty-printer": "~2@dev",
        "memio/twig-template-engine": "~2@dev",
        "memio/validator": "~2@dev",
        "phpunit/phpunit": "~6",
        "symfony/web-server-bundle": "^3.3",
        "behat/behat": "^3.3",
        "php-http/httplug": "^1.1",
        "behat/symfony2-extension": "^2.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Albin Kerouanton",
            "email": "albin.kerouanton@knplabs.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "bin-dir": "bin/"
    },
    "autoload": {
        "psr-4": {
            "NiR\\GhDashboard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NiR\\GhDashboard\\Behat\\": "features/"
        }
    }
}
